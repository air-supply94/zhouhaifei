    "axios": "0.27.2",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0",
    "web-vitals": "3.0.0",
    "register-service-worker": "1.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.165",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^14.14.10",
    "@types/qs": "^6.9.5",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "commitizen": "^4.2.2",
    "eslint": "7.29.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "mockjs": "^1.1.0",
    "postcss": "8.3.11",
    "prettier": "2.2.1",
    "stylelint": "13.13.1",
    "typescript": "4.2.4",
    "validate-commit-msg": "^2.14.0",
    "@zhouhaifei/code-style": "2.22.5",
    "@zhouhaifei/pc-config": "2.22.0",
    "@zhouhaifei/react-scripts": "2.22.3",
    "@zhouhaifei/test": "2.22.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "Chrome >= 54",
    "Firefox >= 47",
    "Edge >= 12",
    "Explorer >= 11",
    "Safari >= 10.1",
    "Opera >= 41"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "git add . && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && validate-commit-msg"
    }
  },
  "lint-staged": {
    "**/*.less": [
      "npm run lint:style-less",
      "git add ."
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint:js",
      "git add ."
    ],
    "**/*.{css,md,html,json}": [
      "prettier --write",
      "git add ."
    ]
  },
  "resolutions": {
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6"
  },
  "scripts": {
    "commit": "cz",
    "start": "zhouhaifei-react-scripts start",
    "build": "zhouhaifei-react-scripts build",
    "test": "zhouhaifei-test react",
    "test-match": "zhouhaifei-test react --testMatch=",
    "lint-staged": "lint-staged --allow-empty",
    "lint:style-less": "stylelint --cache --quiet --fix \"src/**/*.less\" --syntax less",
    "lint:js": "eslint --fix --cache --quiet --ext .js,.jsx,.ts,.tsx .",
    "prettier": "prettier --cache --cache-strategy metadata -c --write \"**/*.{css,md,html,json}\""
  }
}
