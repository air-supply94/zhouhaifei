{
  "dependencies": {
    "antd": "5.2.1",
    "axios": "0.27.2",
    "lodash": "4.17.21",
    "mobx": "5.15.7",
    "mobx-react": "6.3.1",
    "moment": "2.29.4",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0",
    "register-service-worker": "1.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@types/lodash": "^4.14.165",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^14.14.10",
    "@types/qs": "^6.9.5",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.3",
    "eslint": "7.29.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "mockjs": "^1.1.0",
    "postcss": "8.3.11",
    "prettier": "2.2.1",
    "stylelint": "13.13.1",
    "typescript": "4.2.4",
    "validate-commit-msg": "^2.14.0",
    "@zhouhaifei/code-style": "2.27.0",
    "@zhouhaifei/pc-config": "2.27.0",
    "@zhouhaifei/react-scripts": "2.27.0",
    "@zhouhaifei/vite-config": "2.27.0",
    "vite": "^4.4.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 16"
  },
  "browserslist": [
    "> 0.2%",
    "Chrome >= 60",
    "Firefox >= 60",
    "Edge >= 16",
    "Safari >= 10.1"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "git add . && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && validate-commit-msg"
    }
  },
  "lint-staged": {
    "**/*.less": [
      "npm run lint:style-less",
      "git add ."
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint:js",
      "git add ."
    ],
    "**/*.{css,md,html,json}": [
      "prettier --write",
      "git add ."
    ]
  },
  "resolutions": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7"
  },
  "scripts": {
    "commit": "cz",
    "start-vite": "vite",
    "start": "zhouhaifei-react-scripts start",
    "build": "zhouhaifei-react-scripts build",
    "lint-staged": "lint-staged --allow-empty",
    "lint:style-less": "stylelint --cache --quiet --fix \"src/**/*.less\" --syntax less",
    "lint:js": "eslint --fix --cache --quiet --ext .js,.jsx,.ts,.tsx .",
    "prettier": "prettier --cache --cache-strategy metadata -c --write \"**/*.{css,md,html,json}\""
  }
}
