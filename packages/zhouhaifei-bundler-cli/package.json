{
  "name": "@zhouhaifei/bundler-cli",
  "version": "3.1.16",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./dev": {
      "import": {
        "types": "./dist/dev.d.ts",
        "default": "./dist/dev.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.cjs"
      }
    },
    "./build": {
      "import": {
        "types": "./dist/build.d.ts",
        "default": "./dist/build.js"
      },
      "require": {
        "types": "./dist/build.d.ts",
        "default": "./dist/build.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "zhouhaifei-bundler-cli": "bin/zhouhaifei-bundler-cli.js"
  },
  "files": [
    "bin",
    "dist",
    "openChrome.applescript"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@zhouhaifei/babel-preset": "workspace:*",
    "@zhouhaifei/postcss-preset": "workspace:*",
    "babel-loader": "9.1.3",
    "cac": "6.7.14",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "chalk": "4.1.2",
    "copy-webpack-plugin": "11.0.0",
    "cross-spawn": "7.0.3",
    "css-loader": "6.8.1",
    "css-minimizer-webpack-plugin": "5.0.1",
    "dotenv": "16.3.1",
    "dotenv-expand": "10.0.0",
    "esbuild": "0.19.4",
    "esbuild-loader": "4.0.2",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.5.3",
    "less": "4.2.0",
    "less-loader": "11.1.3",
    "mini-css-extract-plugin": "2.7.6",
    "open": "8.4.2",
    "postcss-loader": "7.3.3",
    "preload-webpack-plugin": "3.0.0-beta.4",
    "react-refresh": "0.14.0",
    "react-refresh-webpack-plugin": "0.1.0",
    "sass-loader": "13.3.2",
    "speed-measure-webpack-plugin": "1.5.0",
    "style-loader": "3.3.3",
    "stylus-loader": "7.1.3",
    "terser-webpack-plugin": "5.3.9",
    "unused-webpack-plugin": "2.4.0",
    "webpack": "5.89.0",
    "webpack-5-chain": "8.0.1",
    "webpack-bundle-analyzer": "4.9.1",
    "webpack-dev-server": "4.15.1",
    "webpack-manifest-plugin": "5.0.0",
    "webpackbar": "5.0.2"
  },
  "devDependencies": {
    "tsup": "8.1.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
