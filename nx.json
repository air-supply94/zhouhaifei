{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["test", "build"],
        "parallel": 5
      }
    }
  },
  "targetDefaults": {
    "test": {
      "dependsOn": ["^build"]
    },
    "build": {
      "dependsOn": ["^build", "^prebuild"]
    }
  }
}
